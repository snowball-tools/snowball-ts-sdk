{
  "name": "@snowballtools/snowball-ts-auth",
  "version": "0.1.1",
  "author": "Snowball Tools Inc. <viv@snowballtools.xyz>",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "sideEffects": false,
  "browser": {
    "crypto": false,
    "stream": false
  },
  "files": [
    "dist",
    "src/**/*.ts",
    "!dist/**/*.tsbuildinfo",
    "!vitest.config.ts",
    "!.env",
    "!src/**/*.test.ts",
    "!src/__tests__/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:types",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --outDir ./dist/cjs --removeComments --verbatimModuleSyntax false && echo > ./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --outDir ./dist/esm --removeComments && echo > ./dist/esm/package.json '{\"type\":\"module\"}'",
    "build:types": "tsc --project tsconfig.build.json --module esnext --declarationDir ./dist/types --emitDeclarationOnly --declaration --declarationMap",
    "clean": "rm -rf ./dist",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "description": "snowball auth library",
  "repository": "https://github.com/snowball-tools/snowball-ts-auth",
  "keywords": [
    "ts"
  ],
  "bugs": {
    "url": "https://github.com/snowball-tools/snowball-ts-auth/issues"
  },
  "homepage": "https://github.com/snowball-tools/snowball-ts-auth#readme",
  "private": true,
  "dependencies": {
    "@alchemy/aa-alchemy": "0.1.0-alpha.28",
    "@alchemy/aa-core": "0.1.0-alpha.28",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/wallet": "^5.7.0",
    "@lit-protocol/auth-helpers": "^2.2.50",
    "@lit-protocol/constants": "^2.2.50",
    "@lit-protocol/encryption": "^2.2.50",
    "@lit-protocol/lit-auth-client": "^2.2.50",
    "@lit-protocol/pkp-ethers": "^2.2.50",
    "@lit-protocol/types": "^2.2.50",
    "@simplewebauthn/browser": "^7.2.0",
    "@simplewebauthn/typescript-types": "^7.0.0",
    "alchemy-sdk": "^2.10.0",
    "ethers": "^5.5.1",
    "tsdom": "^0.6.9",
    "viem": "^1.9.3"
  },
  "devDependencies": {
    "@types/node": "^20.5.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.14.25",
    "esbuild-node-builtins": "^0.1.0",
    "eslint": "8.47.0",
    "prettier": "^2.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.0.4",
    "typescript-template": "*",
    "vitest": "^0.31.0"
  }
}
